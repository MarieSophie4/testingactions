    "test-single-headless": "ng test --watch=false --code-coverage --browsers=ChromeHeadless",
    "scss-lint": "stylelint \"**/*.scss\" --fix",
    "lint": "ng lint --fix && npm run scss-lint",
    "compodoc": "npx compodoc -p tsconfig.doc.json -d ./docs"
