{
  "name": "brokerportal",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "postinstall": "ngcc",
    "start": "ng serve",
    "build": "ng build --configuration production",
    "devbuildAWS": "ng build --prod --source-map --base-href /cmtlifeportal/ --deploy-url /cmtlifeportal/",
